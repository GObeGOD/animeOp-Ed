<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.animeopedquiz2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />  

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.animeopedquiz2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".LevelActivity"  
            android:parentActivityName="com.example.animeopedquiz2.MainActivity" 
            android:label="@string/levelTitle">     
        </activity>
        
         <activity android:name=".MusicSelection"  
          android:parentActivityName=".LevelActivity" 
            android:label="@string/musicSelect">
            </activity>
            
           <activity android:name=".MusicPlayActivity"  
          android:parentActivityName=".MusicSelection" 
          android:label="@string/musicPlayActivity">
            </activity>
    </application>

</manifest>
